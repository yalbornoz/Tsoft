pipeline {
         agent any
         stages {
                 stage('Build') {
                 steps {
                     echo 'Hola, estoy construyendo'
                 }
                 }
         }
}

         stages {
                  new File('/C:\Program Files\Jenkins\tsfot.txt').withWriter('UTF-8') { writer ->
                   try {
                            writer << 'hello world\n'
                            } finally {
                             writer.close()
                       }
                  }                     }
}
